{
    "name": "codehq-dk/repo-info-example-plugin",
    "description": "Hello World simple example of af Repository Information Plugin",
    "license": "MIT",
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "codehq-dk/repo-info-contracts": "^v1.0.0-alpha1"
    },
    "autoload": {
        "psr-4": {
            "CodeHqDk\\RepositoryInformation\\ExamplePlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeHqDk\\RepositoryInformation\\ExamplePlugin\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests",
        "test-report": "vendor/bin/phpunit tests/ --coverage-html tests/CoverageReport"
    }
}
